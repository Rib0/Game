{
	"presets": [
		"@babel/preset-env",
		["@babel/preset-react", {
			"runtime": "automatic"
		}]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		[
			"transform-react-remove-prop-types",
			{
				"mode": "wrap",
				"ignoreFilenames": [
					"node_modules"
				]
			}
		],
		"@babel/plugin-transform-react-constant-elements"
	]
}